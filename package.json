{
	"name": "not-error-collect",
	"version": "0.0.22",
	"description": "not-* family module for collecting error reports",
	"main": "index.js",
	"scripts": {
		"test": "./node_modules/.bin/mocha --reporter spec",
		"pretest": "./node_modules/.bin/eslint ./src/**/**",
		"lint": "./node_modules/.bin/eslint ./src/**/**",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"cover": "istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-error-collect.git"
	},
	"keywords": [
		"not-framework",
		"not-node",
		"error-collect"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/interrupter/not-error-collect/issues"
	},
	"homepage": "https://github.com/interrupter/not-error-collect#readme",
	"dependencies": {
		"not-meta": "*"
	},
	"devDependencies": {
		"chai": "*",
		"chai-as-promised": "*",
		"eslint": "^7.2.0",
		"ink-docstrap": "^1.3.2",
		"istanbul": "*",
		"jsdoc": "^3.6.4",
		"mocha": "*"
	}
}
